{
  "name": "incidents",
  "version": "1.2.0",
  "private": true,
  "homepage": "https://www.hamiltonincidents.ca/",
  "engines": {
    "node": ">= 8.0.0"
  },
  "description": "",
  "scripts": {
    "deploy": "cross-env NODE_PATH=src react-scripts build && node src/server.js",
    "client": "cross-env NODE_PATH=src react-scripts start",
    "server": "cross-env NODE_PATH=src node src/server.js",
    "watch": "cross-env NODE_PATH=src nodemon ./src/server.js --ignore public/ --ignore src/client",
    "start": "cross-env NODE_PATH=src start npm run server && react-scripts start",
    "test": "cross-env NODE_PATH=src react-scripts test --env=jsdom",
    "build": "cross-env NODE_PATH=src react-scripts build",
    "eject": "react-scripts eject",
    "prune": "node src/scripts/delete_old.js"
  },
  "dependencies": {
